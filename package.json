    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/test": "^3.4.7",
    "@umijs/yorkie": "^2.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
