    "@ethersproject/providers": "^5.1.0",
    "@types/styled-components": "^5.1.9",
    "@types/web3": "^1.2.2",
    "@umijs/preset-react": "^1.4.8",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "graphql": "^15.5.0",
    "react-intl": "^5.15.5",
    "styled-components": "^5.2.3",
    "umi": "3.4.1",
    "umi-request": "^1.3.5",
    "web3": "^1.3.4"
